<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-2.1.xsd">

    <view-entity entity-name="CountryAndMunicipalities" package="moqui.basic.localized.chile">
        <member-entity entity-alias="COUNTRY" entity-name="moqui.basic.Geo"/>
        <member-entity entity-alias="RA" entity-name="moqui.basic.GeoAssoc" join-from-alias="COUNTRY">
            <key-map field-name="geoId"/>
        </member-entity>
        <member-entity entity-alias="PA" entity-name="moqui.basic.GeoAssoc" join-from-alias="RA">
            <key-map field-name="toGeoId" related="geoId"/>
        </member-entity>
        <member-entity entity-alias="MA" entity-name="moqui.basic.GeoAssoc" join-from-alias="PA">
            <key-map field-name="toGeoId" related="geoId"/>
        </member-entity>
        <member-entity entity-alias="MUNICIPALITY" entity-name="moqui.basic.Geo" join-from-alias="MA">
            <key-map field-name="toGeoId" related="geoId"/>
        </member-entity>
        <alias-all entity-alias="COUNTRY" prefix="country"/>
        <alias-all entity-alias="RA" prefix="ra"/>
        <alias-all entity-alias="PA" prefix="pa"/>
        <alias-all entity-alias="MA" prefix="ma"/>
        <alias-all entity-alias="MUNICIPALITY" prefix="municipality"/>
        <entity-condition>
            <econdition field-name="countryGeoTypeEnumId" value="GEOT_COUNTRY"/>
            <econdition field-name="raGeoAssocTypeEnumId" value="GAT_REGIONS"/>
            <econdition field-name="paGeoAssocTypeEnumId" value="GAT_REGIONS"/>
        </entity-condition>
    </view-entity>

    <view-entity entity-name="InvoiceAndFiscalTaxDocument" package="mchile">
        <member-entity entity-alias="payment" entity-name="mantle.account.payment.Payment"/>
        <member-entity entity-alias="invoice" entity-name="mantle.account.invoice.Invoice" join-from-alias="payment">
            <key-map field-name="forInvoiceId" related="invoiceId"/></member-entity>
        <member-entity entity-alias="taxDoc" entity-name="mch.dte.FiscalTaxDocument" join-from-alias="invoice"
                       join-optional="true"><key-map field-name="invoiceId"/></member-entity>
        <alias-all entity-alias="payment">
            <exclude field="amount"/>
        </alias-all>
        <alias-all entity-alias="invoice">
            <exclude field="fromPartyId"/>
            <exclude field="toPartyId"/>
            <exclude field="statusId"/>
            <exclude field="overrideOrgPartyId"/>
            <exclude field="partyRelationshipId"/>
            <exclude field="timePeriodId"/>
            <exclude field="acctgTransResultEnumId"/>
        </alias-all>
        <alias-all entity-alias="taxDoc">
            <exclude field="invoiceId"/>
        </alias-all>
    </view-entity>

    <view-entity entity-name="FacilityPartyAndPerson" package="mchile">
        <member-entity entity-alias="FP" entity-name="mantle.facility.FacilityParty"/>
        <member-entity entity-alias="P" entity-name="mantle.party.Person" join-from-alias="FP">
            <key-map field-name="partyId"/>
        </member-entity>
        <member-entity entity-alias="FA" entity-name="mantle.facility.Facility" join-from-alias="FP">
            <key-map field-name="facilityId"/>
        </member-entity>
        <alias-all entity-alias="FP"/>
        <alias-all entity-alias="P">
            <exclude field="partyId"/>
        </alias-all>
        <alias name="facilityTypeEnumId" entity-alias="FA"/>
    </view-entity>

    <view-entity entity-name="PhysicalInventoryCountDetails" package="mchile">
        <member-entity entity-alias="PIC" entity-name="mantle.product.asset.PhysicalInventoryCount"/>
        <member-entity entity-alias="PI" entity-name="mantle.product.asset.PhysicalInventory" join-from-alias="PIC">
            <key-map field-name="physicalInventoryId"/>
        </member-entity>
        <member-entity entity-alias="FA" entity-name="mantle.facility.Facility" join-from-alias="PIC">
            <key-map field-name="facilityId"/>
        </member-entity>
        <member-entity entity-alias="LO" entity-name="mantle.facility.FacilityLocation" join-from-alias="PIC">
            <key-map field-name="facilityId"/>
            <key-map field-name="locationSeqId"/>
        </member-entity>
        <member-entity entity-alias="PR" entity-name="mantle.product.Product" join-from-alias="PIC"
                       join-optional="true">
            <key-map field-name="productId"/>
        </member-entity>
        <member-entity entity-alias="AD" entity-name="mantle.product.asset.AssetDetail" join-from-alias="PIC"
                       join-optional="true">
            <key-map field-name="physicalInventoryId"/>
            <key-map field-name="physicalInventoryCountId"/>
        </member-entity>
        <alias-all entity-alias="PIC"/>
        <alias name="partyId" entity-alias="PI"/>
        <alias name="physicalInventoryDate" entity-alias="PI"/>
        <alias name="physicalInventoryStatusId" field="statusId" entity-alias="PI"/>
        <alias name="facilityName" entity-alias="FA"/>
        <alias name="locationDescription" entity-alias="LO" field="description"/>
        <alias name="areaId" entity-alias="LO"/>
        <alias name="aisleId" entity-alias="LO"/>
        <alias name="sectionId" entity-alias="LO"/>
        <alias name="levelId" entity-alias="LO"/>
        <alias name="positionId" entity-alias="LO"/>
        <alias name="productName" entity-alias="PR"/>
        <alias name="quantityOnHandDiff" entity-alias="AD" function="sum"/>
    </view-entity>

    <view-entity entity-name="TillShiftDetail" package="mchile">
        <member-entity entity-alias="TILL" entity-name="mchile.TillShift"/>
        <member-entity entity-alias="FA" entity-name="mantle.account.financial.FinancialAccount" join-from-alias="TILL">
            <key-map field-name="finAccountId"/>
        </member-entity>
        <member-entity entity-alias="UA" entity-name="moqui.security.UserAccount" join-from-alias="FA"
                       join-optional="true">
            <key-map field-name="ownerPartyId" related="partyId"/>
        </member-entity>
        <alias-all entity-alias="TILL"/>
        <alias-all entity-alias="FA">
            <exclude field="finAccountId"/>
            <exclude field="statusId"/>
        </alias-all>
        <alias name="finAccountStatusId" field="statusId" entity-alias="FA"/>
        <alias name="userFullName" entity-alias="UA"/>
    </view-entity>

    <view-entity entity-name="OrderHeaderAndItemAndProduct" package="mchile">
        <member-entity entity-alias="OH" entity-name="mantle.order.OrderHeader"/>
        <member-entity entity-alias="OI" entity-name="mantle.order.OrderItem" join-from-alias="OH">
            <key-map field-name="orderId"/>
        </member-entity>
        <member-entity entity-alias="PR" entity-name="mantle.product.Product" join-from-alias="OI">
            <key-map field-name="productId"/>
        </member-entity>
        <alias-all entity-alias="OI"/>
        <alias-all entity-alias="PR">
            <exclude field="comments"/>
        </alias-all>
        <alias name="placedDate" entity-alias="OH"/>
        <alias name="currencyUomId" entity-alias="OH"/>
        <alias name="productStoreId" entity-alias="OH"/>
        <alias name="enteredByPartyId" entity-alias="OH"/>
        <alias name="orderStatusId" field="statusId" entity-alias="OH"/>
        <alias name="totalAmount" type="number-decimal">
            <complex-alias operator="*">
                <complex-alias-field entity-alias="OI" field="quantity" default-value="0"/>
                <complex-alias-field entity-alias="OI" field="unitAmount" default-value="0"/>
            </complex-alias>
        </alias>
    </view-entity>

    <view-entity entity-name="InvoiceSummaryAndFiscalTaxDocument" package="mchile">
        <member-entity entity-alias="OH" entity-name="mantle.order.OrderHeader"/>
        <member-entity entity-alias="OIB" entity-name="mantle.order.OrderItemBilling" join-from-alias="OH">
            <key-map field-name="orderId"/>
        </member-entity>
        <member-entity entity-alias="INV" entity-name="mantle.account.invoice.Invoice" join-from-alias="OIB">
            <key-map field-name="invoiceId"/>
        </member-entity>
        <member-entity entity-alias="IITM" entity-name="mantle.account.invoice.InvoiceItem" join-from-alias="INV">
            <key-map field-name="invoiceId"/></member-entity>
        <member-entity entity-alias="FTD" entity-name="mch.dte.FiscalTaxDocument" join-from-alias="INV" join-optional="true">
            <key-map field-name="invoiceId"/></member-entity>
        <alias-all entity-alias="FTD"/>
        <alias name="orderId" entity-alias="OH"/>
        <alias name="invoiceId" entity-alias="INV"/>
        <alias name="fromPartyId" entity-alias="INV"/>
        <alias name="toPartyId" entity-alias="INV"/>
        <alias name="invoiceTypeEnumId" entity-alias="INV"/>
        <alias name="statusId" entity-alias="INV"/>
        <alias name="currencyUomId" entity-alias="INV"/>
        <alias name="invoiceDate" entity-alias="INV"><description>For summary use conditions for range, but don't select</description></alias>
        <alias name="invoiceCount" entity-alias="INV" field="invoiceId" function="count-distinct"/>
        <alias name="grandTotal" entity-alias="OH"/>
    </view-entity>

    <view-entity entity-name="ProductStorePaymentGlAccountAndEnumeration" package="mchile">
        <member-entity entity-alias="PSPA" entity-name="ProductStorePaymentGlAccount"/>
        <member-entity entity-alias="P" entity-name="moqui.basic.Enumeration"
                       join-from-alias="PSPA">
            <key-map field-name="paymentInstrumentEnumId" related="enumId"/>
        </member-entity>
        <alias-all entity-alias="PSPA"/>
        <alias-all entity-alias="P"/>
    </view-entity>

    <view-entity entity-name="BankAccountAndPaymentMethod" package="mchile">
        <member-entity entity-alias="BA" entity-name="mantle.account.method.BankAccount"/>
        <member-entity entity-alias="PM" entity-name="mantle.account.method.PaymentMethod" join-from-alias="BA">
            <key-map field-name="paymentMethodId"/>
        </member-entity>
        <alias-all entity-alias="BA"/>
        <alias-all entity-alias="PM"/>
    </view-entity>

    <view-entity entity-name="OrderAndPayment" package="mchile">
        <member-entity entity-alias="OH" entity-name="mantle.order.OrderHeader"/>
        <member-entity entity-alias="PM" entity-name="mantle.account.payment.Payment" join-from-alias="OH" join-optional="true">
            <key-map field-name="orderId"/>
        </member-entity>
        <member-entity entity-alias="FA" entity-name="mantle.account.financial.FinancialAccountTrans" join-from-alias="PM" join-optional="true">
            <key-map field-name="finAccountTransId"/>
        </member-entity>
        <alias-all entity-alias="OH"/>
        <alias-all entity-alias="PM">
            <exclude field="statusId"/>
            <exclude field="visitId"/>
        </alias-all>
        <alias name="financialAccountAmount" entity-alias="FA" field="amount"/>
        <alias name="change" type="number-decimal" default-display="true">
            <complex-alias operator="-">
                <complex-alias-field entity-alias="FA" field="amount"/>
                <complex-alias-field entity-alias="PM" field="amount"/>
            </complex-alias>
        </alias>
    </view-entity>

    <view-entity entity-name="TillShiftDocumentAndPaymentAndCheck" package="mchile">
        <member-entity entity-alias="TSD" entity-name="mchile.TillShiftDocument"/>
        <member-entity entity-alias="P" entity-name="mantle.account.payment.Payment" join-from-alias="TSD">
            <key-map field-name="paymentId"/>
        </member-entity>
        <member-entity entity-alias="CH" entity-name="mantle.account.method.BankAccountCheck" join-from-alias="P" join-optional="true">
            <key-map field-name="paymentId"/>
        </member-entity>
        <member-entity entity-alias="BA" entity-name="mantle.account.method.BankAccount" join-from-alias="CH" join-optional="true">
            <key-map field-name="paymentMethodId"/>
        </member-entity>
        <alias-all entity-alias="TSD"/>
        <alias-all entity-alias="P">
            <exclude field="statusId"/>
        </alias-all>
        <alias name="checkNumber" entity-alias="CH"/>
        <alias name="bankName" entity-alias="BA"/>
    </view-entity>

    <view-entity entity-name="PaymentAndCheck" package="mchile">
        <member-entity entity-alias="P" entity-name="mantle.account.payment.Payment">
            <key-map field-name="paymentId"/>
        </member-entity>
        <!--member-entity entity-alias="CH" entity-name="mchile.BankAccountCheck" join-from-alias="P" join-optional="false"-->
        <member-entity entity-alias="CH" entity-name="mantle.account.method.BankAccountCheck" join-from-alias="P" join-optional="false">
            <key-map field-name="paymentId"/>
        </member-entity>
        <member-entity entity-alias="BA" entity-name="mantle.account.method.BankAccount" join-from-alias="CH" join-optional="false">
            <key-map field-name="paymentMethodId"/>
        </member-entity>
        <alias-all entity-alias="P">
            <exclude field="statusId"/>
        </alias-all>
        <alias name="checkNumber" entity-alias="CH"/>
        <alias name="bankName" entity-alias="BA"/>
        <alias name="transferDocId" entity-alias="CH"/>
    </view-entity>

    <view-entity entity-name="ProductStoreCategory" package="mchile">
        <member-entity entity-alias="PS" entity-name="mantle.product.store.ProductStoreCategory">
            <key-map field-name="productStoreId"/>
            <key-map field-name="productCategoryId"/>
        </member-entity>
        <member-entity entity-alias="PC" entity-name="mantle.product.category.ProductCategory" join-from-alias="PS" join-optional="false">
            <key-map field-name="productCategoryId"/>
        </member-entity>
        <alias-all entity-alias="PS"></alias-all>
        <alias-all entity-alias="PC"></alias-all>
    </view-entity>

    <view-entity entity-name="ProductStoreCategoryMember" package="mchile">
        <member-entity entity-alias="PS" entity-name="mantle.product.store.ProductStoreCategory">
            <key-map field-name="productCategoryId"/>
        </member-entity>
        <member-entity entity-alias="MB" entity-name="mantle.product.category.ProductCategoryMember" join-from-alias="PS" join-optional="false">
            <key-map field-name="productCategoryId"/>
        </member-entity>
        <alias-all entity-alias="PS">
            <exclude field="fromDate"/>
            <exclude field="thruDate"/>
        </alias-all>
        <alias-all entity-alias="MB">
            <exclude field="fromDate"/>
            <exclude field="thruDate"/>
        </alias-all>
        <alias name="storeFromDate" field="fromDate" entity-alias="PS"/>
        <alias name="storeThruDate" field="thruDate" entity-alias="PS"/>
        <alias name="productFromDate" field="fromDate" entity-alias="PS"/>
        <alias name="productThruDate" field="thruDate" entity-alias="PS"/>
    </view-entity>

</entities>
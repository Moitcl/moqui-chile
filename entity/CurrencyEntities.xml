<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-2.1.xsd">

    <entity entity-name="Exchangerate" package="mchile.currency">
        <field name="fromCurrencyUomId" type="id" is-pk="true"/>
        <field name="toCurrencyUomId" type="id" is-pk="true"/>
        <field name="exchangerateTypeEnumId" type="id" is-pk="true" default="MchXrtDefault"/>
        <field name="fromDate" type="date-time" is-pk="true"/>
        <field name="thruDate" type="date-time"/>
        <field name="factor" type="number-decimal" not-null="true"/>
        <relationship type="one" related="moqui.basic.Uom" fk-name="exchangerateFromUomId"><key-map field-name="fromCurrencyUomId" related="uomId"/></relationship>
        <relationship type="one" related="moqui.basic.Uom" fk-name="exchangerateToUomId"><key-map field-name="toCurrencyUomId" related="uomId"/></relationship>
        <relationship type="one" related="moqui.basic.Enumeration" title="ExchangeRateType"><key-map field-name="toCurrencyUomId" related="enumId"/></relationship>
        <seed-data>
            <moqui.basic.EnumerationType enumTypeId="ExchangeRateType" description="Type of Currency Exchange Rate"/>
            <moqui.basic.Enumeration enumId="MchXrtDefault" enumTypeId="ExchangeRateType" description="Default Currency Exchange Rate Type"/>
        </seed-data>
    </entity>

</entities>
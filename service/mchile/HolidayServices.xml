    <service verb="calculate" noun="HChlINDE">
        <implements service="mchile.HolidayServices.calculate#Holiday"/>
        <!-- Leyes 2.977, 19.973 -->
        <actions>
            <!-- Ley 2.977 -->
            <if condition="year &lt; 1916"><return/></if>
            <set field="month" from="9"/>
            <set field="day" from="18"/>
